module opticalpathmanager {
    yang-version 1;
    namespace "urn:opendaylight:params:xml:ns:yang:opticalpathmanager";
    prefix "opticalpathmanager";

    import tapi-path-computation {
        prefix tapi-path-computation;
    }

    revision "2018-01-19" {
        description "Initial revision of opticalpathmanager model";
    }

    rpc compute-p-2-p-path {
        input {
            list sep {
                min-elements 2;
                max-elements 2;
                uses tapi-path-computation:path-service-end-point-g;
                description "none";
            }
            container routing-constraint {
                uses tapi-path-computation:routing-constraint-g;
                description "none";
            }
            container objective-function {
                uses tapi-path-computation:path-objective-function-g;
                description "none";
            }
        }
        output {
            container service {
                uses tapi-path-computation:path-computation-service-g;
                description "none";
            }
        }
    }

}

